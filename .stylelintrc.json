{
  "extends": ["stylelint-config-recommended", "stylelint-config-idiomatic-order"],
  "plugins": ["stylelint-order"],
  "rules": {
    "at-rule-no-unknown": null,
    "color-hex-length": "long",
    "color-named": "never",
    "at-rule-empty-line-before": "always",
    "property-no-vendor-prefix": null
  }
}
